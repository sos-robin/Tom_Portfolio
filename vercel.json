{

    "builds": [
        {
            "src": "Portfolio/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "50mb", "runtime": "python3.9" }
        },
        {
            "src": "static/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/img/(.*)",
            "dest": "/static/img/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Portfolio/wsgi.py"
        }
    ]
}
